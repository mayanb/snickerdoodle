{
  "name": "snickerdoodle",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.4.1",
    "babel-polyfill": "^6.26.0",
    "d3": "^4.11.0",
    "immutability-helper": "^2.1.1",
    "jquery": "^3.1.1",
    "js-file-download": "^0.4.1",
    "jwt-decode": "^2.2.0",
    "mathjs": "^3.20.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "npm-run-all": "^4.1.2",
    "qrcodejs": "^1.0.0",
    "raven-for-redux": "^1.3.0",
    "raven-js": "^3.24.0",
    "react": "^15.6.1",
    "react-date-range": "^0.9.2",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^15.6.1",
    "react-draggable": "^2.2.3",
    "react-emojione": "^5.0.0",
    "react-image-fallback": "^4.0.1",
    "react-infinite-scroller": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.0.0",
    "react-scripts": "1.0.13",
    "react-select": "^1.0.0-rc.2",
    "react-spinkit": "^3.0.0",
    "react-toolbox": "^1.3.4",
    "react-transition-group": "^1.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.6.0",
    "svg-intersections": "^0.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-less": "node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-less": "npm run build-less && node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch-less start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css watch-less build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "0.0.3"
  },
  "theme": {
    "primary-color": "#ff0000"
  }
}
